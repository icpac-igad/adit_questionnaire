# Docker Compose Environment Variables
# Copy this file to .env and fill in the values before running docker-compose

# Database Configuration (REQUIRED)
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_USER=postgres
POSTGRES_DB=adit_questionnaire

# Environment
NODE_ENV=production

# CORS Configuration
# Set this to your actual frontend domain in production
CORS_ORIGIN=http://localhost

# Port Configuration (optional - defaults shown)
# DB_EXTERNAL_PORT=3732
# BACKEND_EXTERNAL_PORT=3733
# FRONTEND_EXTERNAL_PORT=3734
